{
  "name": "utrust-ts-library",
  "version": "1.0.0",
  "description": "Utrust library for store integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/utrustdev/utrust-ts-library.git"
  },
  "keywords": [
    "Utrust",
    "library"
  ],
  "author": "utrustdev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/utrustdev/utrust-ts-library/issues"
  },
  "homepage": "https://github.com/utrustdev/utrust-ts-library#readme",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "jest": "^26.6.3",
    "jest-mock-axios": "^4.2.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@types/crypto-js": "^4.0.1",
    "@types/express": "^4.17.11",
    "axios": "^0.21.0",
    "axios-debug-log": "^0.8.2",
    "crypto-js": "^4.0.0"
  }
}
